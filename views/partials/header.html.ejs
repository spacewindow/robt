<body class="<%= pageData.current.id %>">

  <% include ../../images/icons.svg %>

  <div class="app__body">
    <style>
      h1,h2,h3{
        color: <%= pageData.current.color %>;
      }
      .project__header__title h1,
      .project__header__title h2,
      .project__header__title h3,
      .project__header__title p{
        color: white;
      }

      a{
        color: <%= pageData.current.color %>;
      }

    </style>

    <section class="project__header grid" style="background: <%= pageData.current.color %>">
      <div class="grid-cell grid5 project__header__title">
        <header>
          <h1><%= pageData.current.title %></h1>
          <h3><%= pageData.current.client %></h3>
          <p></p>
        </header>
        <p><%= pageData.current.description %></p>
      </div>
      <div class="grid-cell grid7 project__header__hero">
        <img src="./images/hero-<%= pageData.current.id %>.png" alt="">
      </div>
    </section>
